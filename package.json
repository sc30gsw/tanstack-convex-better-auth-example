{
  "name": "-t",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "bunx convex dev --once && concurrently -r npm:dev:web npm:dev:convex",
    "dev:web": "vite dev",
    "dev:ts": "tsc -b -w",
    "dev:convex": "bunx convex dev",
    "build": "vite build && tsc --noEmit",
    "start": "node .output/server/index.mjs",
    "tsc": "tsc --noEmit",
